{
  "name": "ng-float",
  "main": "dist/ng-float.cjs.js",
  "module": "dist/ng-float.esm.js",
  "browser": "dist/ng-float.umd.js",
  "version": "2.0.0",
  "description": "An AngularJS library for building floating draggable layouts",
  "scripts": {
    "build": "rollup --config rollup.prod.config.js",
    "start": "rollup --config rollup.dev.config.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git"
  },
  "author": "Nalin Ilango",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "angular": "^1",
    "jquery-ui": "^1",
    "lodash": "^4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.3.0",
    "cssnano": "^4.1.10",
    "jquery": "^3.5.1",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.33.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "browserslist": [
    "defaults"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parser": "@babel/eslint-parser",
    "env": {
      "browser": true
    },
    "ignorePatterns": [
      "dist/*",
      "public/*"
    ]
  }
